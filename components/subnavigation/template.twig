<nav
    data-module="sb-subnavigation"
    aria-label="{{ params.label }}"
    class="sb-subnavigation {%- if params.classes %} {{ params.classes }}{% endif %}"
    {%- for attribute, value in params.attributes %} {{ attribute }}="{{ value }}"{% endfor %}>
    <ul class="sb-subnavigation__list">
    	{%- for item in params.items %}
	    	<li class="sb-subnavigation__item {{ item.classes | join(' ') }}">
    			<a class="sb-subnavigation__link" href="{{ item.link }}" target="{{ item.target }}" {%- if item.current %} aria-current="page"{% endif %}>
    				{{- item.title -}}
    			</a>
    		</li>
    	{%- endfor %}
    </ul>
</nav>
