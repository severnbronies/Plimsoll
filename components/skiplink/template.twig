<a
    data-module="sb-skiplink"
    href="{{ params.href }}"
    class="sb-skiplink {%- if params.classes %} {{ params.classes }}{% endif %}"
    {%- for attribute, value in params.attributes %} {{ attribute }}="{{ value }}"{% endfor %}>
    {%- if params.html %}{{ params.html }}{% else %}{{ params.text }}{% endif %}
</a>
